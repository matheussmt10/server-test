{
    "name": "back-movie-generator",
    "version": "1.0.0",
    "description": "",
    "main": "build/index.js",
    "scripts": {
        "dev": "NODE_PATH=./src ts-node-dev --respawn ./src/index.ts",
        "test": "./node_modules/.bin/jest --ci --verbose --runInBand --detectOpenHandles --collectCoverage=true '/specs/units-tests' --reporters=default --reporters=jest-junit",
        "build": "npx tsc"
    },
    "keywords": [],
    "author": "Lucas Krolikowski Bento",
    "license": "MIT",
    "jest": {
        "verbose": true,
        "collectCoverageFrom": [
            "./src/**/*.*",
            "!**/node_modules/**",
            "!**/vendor/**",
            "!**/index.ts"
        ],
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "testRegex": "^.+\\.spec\\.ts$",
        "testEnvironment": "node"
    },
    "dependencies": {
        "body-parser": "^1.20.0",
        "cors": "^2.8.5",
        "express": "^4.18.1",
        "pg": "^8.7.3",
        "reflect-metadata": "^0.1.13",
        "sequelize": "^6.21.3",
        "sequelize-typescript": "^2.1.3"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jest": "^28.1.6",
        "@types/node": "^18.6.2",
        "@types/pg": "^8.6.5",
        "@types/sequelize": "^4.28.14",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.7.4"
    }
}
